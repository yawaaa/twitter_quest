    <p><%= current_user.name %> @<%= current_user.twitter_ID %></p>
    <p><%= current_user.description %></p>
    <%= current_user.listed_count %>
    <p>
    ツイート:<%= current_user.tweet_count %>
    </p>
    <p>
    フォロー:<%= current_user.followings_count %>
    </p>
